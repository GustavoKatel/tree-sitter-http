{
  "name": "http",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "request"
        },
        {
          "type": "SYMBOL",
          "name": "header"
        },
        {
          "type": "SYMBOL",
          "name": "json_body"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "request": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "method",
          "content": {
            "type": "SYMBOL",
            "name": "method"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_whitespace"
        },
        {
          "type": "FIELD",
          "name": "url",
          "content": {
            "type": "SYMBOL",
            "name": "url"
          }
        }
      ]
    },
    "method": {
      "type": "PATTERN",
      "value": "(?:GET|POST|PATCH|DELETE|PUT)"
    },
    "url": {
      "type": "PATTERN",
      "value": "(www|http:|https:)\\/\\/(?:w{1,3}\\.)?[^\\s.]+(?:\\.[a-z]+)*(?::\\d+)?([\\w.,@?^=%&amp;:\\/~+#-]*[\\w@?^=%&amp;\\/~+#-])?"
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "name"
              },
              {
                "type": "STRING",
                "value": ":"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "_whitespace"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "value"
          }
        }
      ]
    },
    "name": {
      "type": "PATTERN",
      "value": "[A-Za-z-]+"
    },
    "value": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "json_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "start",
          "content": {
            "type": "SYMBOL",
            "name": "json_body_start"
          }
        },
        {
          "type": "PATTERN",
          "value": ".*"
        },
        {
          "type": "FIELD",
          "name": "end",
          "content": {
            "type": "SYMBOL",
            "name": "json_body_end"
          }
        }
      ]
    },
    "json_body_start": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "{"
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          }
        ]
      }
    },
    "json_body_end": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "}"
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "_whitespace": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "[\\t\\v ]"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
